.left_img_class {
    width: 32rpx !important; height: 44rpx !important;
}
.tab {
    position: fixed;
    top: 0; left: 0;
    display: flex; flex-direction: row; align-content: center; justify-content: center;
    width: 100%; padding: 20rpx 0;
    text-align: center;
    z-index: 2; 
    border-bottom: 1rpx solid #f2f2f2;
    background-color: #fff;
}
.tab>view {
    flex: 1;
    display: inline-block;
    position: relative;
    vertical-align: top; text-align: center;
}
.tab>view>text:nth-child(1) {
    font-size: 28rpx; color: #999; line-height: 40rpx;
}
.tab>.act>text:nth-child(1) {
    font-size: 28rpx; color: #333;
    font-weight: bold;
}
.tab>.act>view:nth-child(2) {
    position: absolute;
    bottom: -10rpx; left: 0; right: 0;
    margin: auto;
    width: 28rpx; height: 4rpx;
    background-color: #333;
}
.md_margin_live{
    padding: 0 24rpx;
}
#main {
    width: auto;
}
#main>swiper-item {
    width: 750rpx;
}
.content .list {
    position: relative;
    height: 264rpx; width: 750rpx;
    border-bottom: 1rpx solid #f2f2f2;
}
.content .list .live_bgImg_box{
    position: absolute;
    top: 32rpx; left: 24rpx;
    width: 168rpx; height: 200rpx;
    overflow: hidden;
    border-radius: 8rpx;
}
.content .list .live_bgImg {
    width: 168rpx; height: 200rpx;
}
.content .list>text {
    position: absolute;
    top: 40rpx; left: 0;
    height: 30rpx; width: 82rpx;
    line-height: 30rpx; text-align: center;
    font-size: 20rpx; color: #fff;
    background-color: rgba(237, 101, 145, 0.6);
    border-top-left-radius: 8rpx;
    border-bottom-right-radius: 8rpx;
}
.content .list  .vipType{
    position: absolute;
    left: 28rpx; top: 36rpx;
    width: 44rpx; height: 26rpx;
}
.content .list .liveType{
    display: flex; flex-direction: row; align-items: center; justify-content: center;
    position: absolute;
    right: 0rpx; bottom: 0rpx;
    width: 118rpx; height: 30rpx;
    border-top-left-radius: 30rpx;
    border-bottom-left-radius: 30rpx;
}
.content .list .liveType>view:nth-child(1) {
    display: flex; flex-direction: row; align-items: center;
    width: 46rpx; height: 30rpx;
    padding-left: 14rpx;
    border-top-left-radius: 30rpx;
    border-bottom-left-radius: 30rpx;
    background-color: #ff2c79;
}
.content .list .liveType>image:nth-child(1) {
    width: 46rpx; height: 30rpx;
}
.content .list .liveType>view:nth-child(2) {
    position: relative;
    display: flex; flex-direction: row; align-items: center; justify-content: center;
    width: 72rpx; height: 30rpx;
    line-height: 30rpx;
    background-color: rgba(0, 0, 0, 0.6)
}
.content .list .liveType text{
    display: inline-block;
    height: 18rpx; line-height: 18rpx;
    font-size: 18rpx; color: #fff;
}
.content .list .liveType>view image{
    margin-right: 4rpx;
    height: 22rpx; width: 22rpx;
}
.content .list .end{
    background-color: rgba(0, 0, 0, 0.4);
}
.content .list>.liveContent{
    position: absolute;
    top: 0; right: 24rpx;
    padding-top: 26rpx;
    width: 510rpx; height: 238rpx;
}
.content .list>view>view:nth-child(1) {
    display: -webkit-box; word-break: break-all;
    width: 510rpx;
    text-overflow: -o-ellipsis-lastline;overflow: hidden;text-overflow: ellipsis;
    -webkit-line-clamp: 2; -webkit-box-orient: vertical; line-clamp: 2;
}
.content .list>view>view:nth-child(1)>.typeLive {
    position: relative;
    top: -2rpx;
    display: inline-block;
    margin-right: 16rpx;
    text-align: center; line-height: 28rpx;
    font-size: 20rpx;
    border-radius: 4rpx;
}
.content .list>view>view:nth-child(1)>.liveName {
    font-size: 28rpx; 
    line-height: 40rpx;
}
.content .list>.liveContent>view:nth-child(2) {
    position: absolute;
    bottom: 64rpx; left: 0;
    width: 452rpx; height: 36rpx;
    font-size: 0;
}
.content .list>.liveContent>view:nth-child(2)>text {
    display: inline-block;
    height: 36rpx; min-width: 68rpx; 
    padding: 0 10rpx; margin-right: 16rpx;
    line-height: 36rpx; text-align: center;
    font-size: 22rpx; color: #666;
    background-color: #F6F6F6;
    border-radius: 4rpx;
}
.content .list>.liveContent>.num {
    position: absolute;
    bottom: 60rpx; right: 0;
    font-size: 26rpx;
    color: #999;
}
.content .list .nofree, .content .list .timefree {
    position: absolute;
    bottom: 0rpx; left: 0;
    width: 510rpx;
}
.content .list .nofree>text:nth-child(1) {
    font-size: 24rpx; color: #ff2c79;
}
.content .list .nofree>text:nth-child(2) {
    margin-left: 6rpx;
    font-size: 32rpx; font-weight: bold;
    color: #ff2c79;
}
.content .list .timefree>text:nth-child(1) {
    font-size: 28rpx; color: #ff2c79;
}
.content .list .nofree>text:nth-child(3) {
    margin-left: 12rpx;
    font-size: 26rpx;
    color: #ff2c79;
}
.content .list .nofree_ios>text:nth-child(3) {
    margin-left: 0;
    font-size: 28rpx;
}
.content .list .oriprice {
    font-size: 26rpx; color: #999 !important; 
    text-decoration: line-through;
}
.content .list .timefree>.oriprice {
    margin-left: 8rpx;
    font-size: 26rpx; color: #999 !important; 
    text-decoration: line-through;
}
.content .list .nofree .see {
    position: absolute;
    bottom: 0; right: 0; top: 0;
    margin: auto;
    font-size: 26rpx;
    color: #ff2c79;
}
.content .list .free {
    position: absolute;
    bottom: 0rpx; left: 0;
    font-size: 28rpx; color: #ff2c79;
}
.content .list .free_buy{
    font-size: 26rpx
}
.notLive{
    display: flex; flex-direction: column; align-items: center;
    margin-top: 240rpx;
}
.notLive image{
    height: 218rpx; width: 300rpx;
}
.notLive text{
    margin-top: 32rpx;
    font-size: 28rpx;line-height: 40rpx;
    color: #666
}
.loading text{
    display: inline-block;
    margin-right: 20rpx;
    line-height: 60rpx; 
    color: #999; font-size: 22rpx
}
.loading{
    height: 60rpx; margin-top: 30rpx;
    text-align:  center;
}
.loading text:nth-child(2){
    color: #ccc
}

.page_bottom {
    position: fixed;
    bottom: 0; left: 0;
    width: 100%; height: 24rpx;
    z-index: 20;
    background:linear-gradient(to top,rgba(102, 102, 102, 0.06) 0%,rgba(255,255,255,0) 24rpx);
}